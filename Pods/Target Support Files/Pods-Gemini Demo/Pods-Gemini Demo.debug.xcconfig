ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/DragRotateScaleView" "${PODS_CONFIGURATION_BUILD_DIR}/EPSignature" "${PODS_CONFIGURATION_BUILD_DIR}/Gemini" "${PODS_CONFIGURATION_BUILD_DIR}/WeScan"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "${PODS_CONFIGURATION_BUILD_DIR}/DragRotateScaleView/DragRotateScaleView.framework/Headers" -iquote "${PODS_CONFIGURATION_BUILD_DIR}/EPSignature/EPSignature.framework/Headers" -iquote "${PODS_CONFIGURATION_BUILD_DIR}/Gemini/Gemini.framework/Headers" -iquote "${PODS_CONFIGURATION_BUILD_DIR}/WeScan/WeScan.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "DragRotateScaleView" -framework "EPSignature" -framework "Gemini" -framework "WeScan"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
